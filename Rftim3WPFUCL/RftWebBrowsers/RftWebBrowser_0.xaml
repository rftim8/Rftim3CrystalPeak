<UserControl x:Class="Rftim3WPFUCL.RftWebBrowsers.RftWebBrowser_0"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Rftim3WPFUCL.RftWebBrowsers"
             mc:Ignorable="d">
    
    <DockPanel Background="{DynamicResource RftBrushBackgroundWindow}">
        <Label DockPanel.Dock="Top"
               Height="50"
               FontSize="25"
               HorizontalAlignment="Center"
               VerticalAlignment="Center"
               Foreground="{DynamicResource RftBrushForegroundWindow}">
            Web Browser
        </Label>
        <Grid>
            <WebBrowser x:Name="wb"
                        Height="500"
                        Width="500"
                        >
            </WebBrowser>
        </Grid>
    </DockPanel>
</UserControl>
