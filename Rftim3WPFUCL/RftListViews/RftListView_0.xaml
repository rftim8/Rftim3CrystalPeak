<UserControl x:Class="Rftim3WPFUCL.RftListViews.RftListView_0"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Rftim3WPFUCL.RftListViews"
             mc:Ignorable="d">

    <DockPanel Background="{DynamicResource RftBrushBackgroundWindow}">
        <Label DockPanel.Dock="Top"
               Height="50"
               FontSize="25"
               HorizontalAlignment="Center"
               VerticalAlignment="Center"
               Foreground="{DynamicResource RftBrushForegroundWindow}">
            List View
        </Label>
        <Grid>
            <ListView>
                <ListViewItem Height="400"/>
                <ListViewItem Height="400"/>
                <ListViewItem Height="400"/>
                <ListViewItem Height="400"/>
                <ListViewItem/>
                <ListViewItem/>
            </ListView>
        </Grid>
    </DockPanel>
</UserControl>
